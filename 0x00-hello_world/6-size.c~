#include <stdio.h>

/**
 * main- Entry point
 * Description: Prints out sizes of types
 *
 * Return: 0 if successful
 */
int main(void)
{
	char c;
	int i;
	long int l;
	long long int n;
	float f;

	printf("Size of a char: %ld byte(s)\n", (unsigned long)sizeof(c));
	printf("Size of an int: %ld byte(s)\n", (unsigned long)sizeof(i));
	printf("Size of a long int: %ld byte(s)\n", (unsigned long)sizeof(l));
	printf("Size of a long long int: %ld byte(s)\n", (unsigned long)sizeof(n));
	printf("Size of a float: %ld byte(s)\n", sizeof(f));
	return (0);
}
